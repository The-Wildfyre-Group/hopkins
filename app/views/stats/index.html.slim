.table-responsive
  table.census-tbl[border="1" style="background-color:white;border:1px solid #CCC;border-collapse:collapse;"]
    caption
      em
    tr
      th[colspan="8"]
        | Proportion and Number of Black Males by Age, Education and Region of the United States*
    tr
      th.blu-bg[colspan="4"]
        |  East
      th.blu-bg[colspan="4"]
        | South
    tr
      th
      - [0, 1, 2, 0, 1, 2].each do |i|
        th
          = education[i]
    tr
      th
        = range[0].first.first
        - (0..2).each do |i|
          td= @data[i].first
        - (3..5).each do |i|
          td= @data[i].first
    tr
      th
        = range[1].first.first
        - (6..8).each do |i|
          td= @data[i].first
        - (9..11).each do |i|
          td= @data[i].first
    tr
      th
        = range[2].first.first
        - (12..14).each do |i|
          td= @data[i].first
        - (15..17).each do |i|
          td= @data[i].first
    tr.blu-bg[rowspan="1"]
      th[colspan="1"]
      th[colspan="3"]
        = regions[2]
      /th[rowspan="1"]
      th[colspan="3"]
        = regions[3]
    tr
      th
        = range[0].first.first
        - (18..20).each do |i|
          td= @data[i].first
        - (21..23).each do |i|
          td= @data[i].first
    tr
      th
        = range[1].first.first
        - (24..26).each do |i|
          td= @data[i].first
        - (27..29).each do |i|
          td= @data[i].first
    tr
      th
        = range[2].first.first
        - (30..32).each do |i|
          td= @data[i].first
        - (33..35).each do |i|
          td= @data[i].first


