/! End bg_gray_container
#newsletter_container.container_gray_bg
  .container.margin_60
    .row
      .col-md-12.col-sm-12.text-center
        .main_title
          /! <h2>Follow Us</h2
          p
            | Follow us on your favorite social platforms.
        .row
          = link_to main_share_fb_link, :target => "_blank" do 
            span.col-lg-3.col-md-3.col-sm-3.col-xs-3.text-center
              = image_tag("icons/facebook.png", alt: "", class: "img-responsive")
          = link_to main_share_twitter_link, :target => "_blank" do 
            span.col-lg-3.col-md-3.col-sm-3.col-xs-3.text-center
              = image_tag("icons/twitter.png", alt: "", class: "img-responsive")
          = link_to "https://www.instagram.com/bmhealthsurvey/", :target => "_blank" do    
            span.col-lg-3.col-md-3.col-sm-3.col-xs-3.text-center
              = image_tag("icons/instagram.png", alt: "", class: "img-responsive")
          = link_to "https://www.youtube.com/channel/UCzeUzAe9h-wZoo37rEtk2oA", :target => "_blank" do    
            span.col-lg-3.col-md-3.col-sm-3.col-xs-3.text-center
              = image_tag("icons/youtube.png", alt: "", class: "img-responsive")
/! End newsletter_container
