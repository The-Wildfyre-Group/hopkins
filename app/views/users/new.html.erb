<h1>Participate</h1> 

<%= form_for @user, url: users_path do |form| %>

  <%= form.label :first_name %>
  <%= form.text_field :first_name %></br>
  
  <%= form.label :last_name %>
  <%= form.text_field :last_name %></br>
  
  <%= form.label :state %>
  <%= form.select :state, Location.states %></br>
  
  <%= form.label :education %>
  <%= form.select :education, education %></br>
  
  <%= form.label :email %>
  <%= form.text_field :email %></br>
  
  <%= form.label :password %>
  <%= form.text_field :password %></br>
  </br>

  <%= form.submit "Participate" %> 

<%end%>