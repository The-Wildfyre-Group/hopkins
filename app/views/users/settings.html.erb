<%= form_for current_user do |f| %>

<%= f.label :first_name %>
<%= f.text_field :first_name %></br></br>

<%= f.label :last_name %>
<%= f.text_field :last_name %></br></br>

<%= f.label :email %>
<%= f.text_field :email %></br></br>

<%= f.label :education %>
<%= f.select :education, education, {:selected=> current_user.education} %></br></br>

<%= f.label :state %>
<%= f.select :state, Location.states, {:selected=> current_user.state} %></br></br>

<%= f.submit %>



<% end %>